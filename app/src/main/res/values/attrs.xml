<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DataWithHint">
        <attr name="subtitle" format="string" />
        <attr name="subtitleTextColor" format="color" />
        <attr name="size" format="enum">
            <enum name="Small" value="0" />
            <enum name="Medium" value="1" />
            <enum name="Large" value="2" />
        </attr>
        <attr name="textColorStyle" format="enum">
            <enum name="Light" value="0" />
            <enum name="Dark" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="DataWithBarView">
        <attr name="rightLabel" format="string" />
        <attr name="fieldLabel" format="string" />
    </declare-styleable>
    <declare-styleable name="FillerBar">
        <attr name="android:color" />
        <attr name="barPercentage" format="integer" />
    </declare-styleable>
    <declare-styleable name="BottomToolbarButton">
        <attr name="android:src" />
        <attr name="android:text" />
        <attr name="showText" format="boolean" />
    </declare-styleable>
    <declare-styleable name="BottomToolbarLayout">
        <attr name="label1Text" format="string" />
        <attr name="label2Text" format="string" />
        <attr name="showHome" format="boolean" />
        <attr name="showSettings" format="boolean" />
        <attr name="fabIcon" format="reference" />
        <attr name="label1Icon" format="reference" />
        <attr name="label2Icon" format="reference" />
    </declare-styleable>
</resources>